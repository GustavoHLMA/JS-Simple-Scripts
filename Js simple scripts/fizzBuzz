let cont = 0;

while (cont <= 100) {
  if (cont % 3 === 0) {
    if (cont % 5 === 0) {
      console.log("fizbuzz");
      cont = cont+1;
      
    } else {
      console.log("fizz");
      cont = cont+1;
    }
  } else if (cont % 5 === 0) {
      console.log("buzz");
      cont = cont+1;

  } else {
    console.log(cont);
    cont = cont+1;
  }
}